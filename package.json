{
  "name": "My-PhotoHub",
  "version": "1.0.0",
  "description": "My Photohub is a web app that makes it easy to share your photos on the web.",
  "source":"index.js",
  "scripts": {
    "predeploy": "del dist && parcel build --dist-dir dist",
    "deploy": "gh-pages -d dist",
    "start": "parcel index.html --open",
    "build": "parcel build --dist-dir dist"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.10",
    "bootstrap": "^5.2.2",
    "parcel": "^2.7.0",
    "photoswipe": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "gh-pages": "^4.0.0"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "javascript",
    "starter"
  ],
  "targets": {
    "default": {
      "includeNodeModules": true
    }
  }
  
}
